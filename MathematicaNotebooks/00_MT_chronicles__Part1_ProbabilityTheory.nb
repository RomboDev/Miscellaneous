     RowBox[{"x", "-", "y"}], "]"}], "<", "10"}], ",", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"x", ",", "y"}], "}"}], "\[Distributed]", 
    RowBox[{"UniformDistribution", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "60"}], "}"}], ",", 
