      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "6"]},
     {
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "6"]},
     {
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "6"]},
     {
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "36"], 
      FractionBox["1", "6"]},
     {
      FractionBox["1", "36"], 
