Cell[BoxData[{
 RowBox[{"Clear", "[", "scores", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"scores", " ", "=", " ", 
   RowBox[{"List", "[", 
    RowBox[{
    "45", ",", "33", ",", "24", ",", "42", ",", "55", ",", "58", ",", "22", 
     ",", "18", ",", "30", ",", "48"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Mean", "[", "scores", "]"}], "//", "N"}]}], "Input",
 CellChangeTimes->{{3.800898187843603*^9, 
