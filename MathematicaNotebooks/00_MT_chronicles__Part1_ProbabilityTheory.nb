Cell[BoxData[
 RowBox[{
  RowBox[{"CDF", "[", 
   RowBox[{
    RowBox[{"NormalDistribution", "[", 
     RowBox[{"300", ",", "50"}], "]"}], ",", "365"}], "]"}], "//", 
  "N"}]], "Input",
 CellChangeTimes->{{3.8009832929360323`*^9, 
  3.800983317610075*^9}},ExpressionUUID->"4f689b3b-71d0-4beb-bba7-\
9a5e3c1f0642"],

