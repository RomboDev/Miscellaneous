    RowBox[{
     RowBox[{"Var", "[", "Y", "]"}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"E", "[", "X", "]"}], ")"}], "2"]}]}], TraditionalForm]],
  FontColor->RGBColor[1, 0.5, 0],ExpressionUUID->
  "84f97eda-e9ea-4855-a173-f895c68f81dc"],
 StyleBox["\nVar[Z] = Var[X/Y] = Var[X(1/Y) ",
  FontColor->RGBColor[1, 0.5, 0]],
 "If independent",
 StyleBox["  Var[X/Y] = ",
  FontColor->RGBColor[1, 0.5, 0]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"E", "[", 
      SuperscriptBox["X", "2"], "]"}], 
     RowBox[{"E", "[", 
