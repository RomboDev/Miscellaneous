      RowBox[{
       RowBox[{
        RowBox[{
        "result", " ", "is", " ", "in", " ", "months", " ", "for", " ", 
         "standard", " ", "normal", " ", "dist"}], ".."}], " ", "close", " ", 
       "to", " ", "3", "std"}], ".."}], " ", "9", "*", "3"}], "=", "27"}], 
   "*)"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{"122", "-", "26.7"}], ")"}], "/", "12"}], "//", "N"}], " ", 
  RowBox[{"(*", 
   RowBox[{"min", " ", "years", " ", "of", " ", "expected", " ", "life"}], 
