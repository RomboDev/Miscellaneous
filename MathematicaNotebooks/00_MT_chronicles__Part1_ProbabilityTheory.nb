Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "x", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"pdf", "=", 
    RowBox[{"Piecewise", "[", 
     RowBox[{"{", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"k", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["x", "2"], "+", "x"}], ")"}]}], ",", 
        RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", "1"}]}], "}"}], 
      "}"}], "]"}]}], ";"}], "                ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "need", " ", "to", " ", "get", " ", "k", " ", "in", " ", "a", " ", "way", 
    " ", "that", " ", "the", " ", "piecewise", " ", "integrates", " ", "to", 
    " ", "1", " ", "first"}], "*)"}], "\[IndentingNewLine]", 
  "                                                                           \
                           ", 
  RowBox[{"(*", " ", 
   RowBox[{"see", " ", "part3", " ", "Normalizing", " ", "Constant"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Integrate", "[", 
   RowBox[{"pdf", ",", "x"}], "]"}], 
  "                                                                ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "here", " ", "we", " ", "see", " ", "that", " ", "for", " ", "1", " ", 
    "it", " ", "is", " ", "5", 
    RowBox[{"k", "/", "6"}], " ", "so", " ", "k", " ", "for", " ", "the", " ",
     "pdf", " ", "is", " ", 
    RowBox[{"6", "/", "5"}]}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"pdf", "=", 
    RowBox[{"Piecewise", "[", 
     RowBox[{"{", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"6", "/", "5"}], 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["x", "2"], "+", "x"}], ")"}]}], ",", 
        RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", "1"}]}], "}"}], 
      "}"}], "]"}]}], ";"}], "           ", 
  RowBox[{"(*", 
   RowBox[{" ", 
    RowBox[{
     RowBox[{
     "now", " ", "we", " ", "can", " ", "use", " ", "it", " ", "as", " ", "a",
       " ", "pdf", " ", "coz", " ", 
      RowBox[{"it", "'"}], "s", " ", "actually", " ", "a", " ", "pdf"}], " ", 
     ";"}], ")"}], " ", "*)"}]}], "\[IndentingNewLine]", 
