Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Clear", "[", 
    RowBox[{"\[Mu]", ",", "\[Sigma]", ",", "prob", ",", "mu", ",", "sigma"}], 
    "]"}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"convert", " ", "years", " ", "to", " ", "months"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"\[Mu]", "=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"10", "*", "12"}], ")"}], "+", "2"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Sigma]", "=", "9"}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"rig", " ", "the", " ", "distribution"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[ScriptCapitalD]", "=", 
    RowBox[{"NormalDistribution", "[", 
     RowBox[{"\[Mu]", ",", "\[Sigma]"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"plot", " ", "ta", " ", "shit"}], "*)"}]}], "\[IndentingNewLine]", 

 RowBox[{
  RowBox[{"Plot", "[", 
   RowBox[{
    RowBox[{"PDF", "[", 
     RowBox[{"\[ScriptCapitalD]", ",", "x"}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "80", ",", "160"}], "}"}], ",", 
    RowBox[{"PlotRange", "\[Rule]", "Full"}]}], "]"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Quiet", "[", 
   RowBox[{"Solve", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"Erf", "[", 
        FractionBox[
         RowBox[{
          RowBox[{"-", "x"}], "+", "\[Micro]"}], 
         RowBox[{
          SqrtBox["2"], " ", "\[Sigma]"}]], "]"}]}], "\[Equal]", 
      RowBox[{"0.997", "/", "2"}]}], ",", "x"}], "]"}], "]"}], 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
